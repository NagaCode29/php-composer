{
    "name": "sinagadiory/php-composer",
    "description": "Belajar PHP Composer",
    "autoload": {
        "psr-4": {
            "Sinagadiory\\PhpComposer\\": "src/"
        },
        "files": [
            "src/Libs/math.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Sinagadiory\\PhpComposer\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Diory Pribadi Sinaga",
            "email": "dioripribadisinaga@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ProgrammerZamanNow/belajar-php-composer-library"
        }
    ],
    "require": {
        "markrogoyski/math-php": "^2.10",
        "programmerzamannow/belajar-php-composer-library": ">=1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=11.4.3"
    },
    "scripts": {
        "jalankan": "php public/index.php"
    }
}
